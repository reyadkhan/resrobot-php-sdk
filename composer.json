{
  "name": "trafiklab/resrobot-php-sdk",
  "description": "Use Swedish public transport data in your project with ResRobot",
  "type": "library",
  "license": "",
  "authors": [
    {
      "name": "Bert Marcelis",
      "email": "bert.marcelis@samtrafiken.se"
    }
  ],
  "keywords": [
    "trafiklab",
    "php",
    "sdk",
    "api",
    "public transport"
  ],
  "homepage": "https://trafiklab.se",
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "cache/apcu-adapter": "^1.0",
    "cache/array-adapter": "^1.0",
    "cache/filesystem-adapter": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "squizlabs/php_codesniffer": "^3.4",
    "phpmd/phpmd": "^2.6.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "Trafiklab\\": "src/Trafiklab/"
    }
  },
  "autoload-dev": {
    "autoload": {
      "psr-4": {
        "Trafiklab\\": [
          "src/Trafiklab/",
          "tests/Trafiklab/"
        ]
      }
    }
  }
}
